{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "SettingsDataConnection": "Data Source=(local);Database=AuroraPlatform;Integrated Security=True"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticsearchConfiguration": {
    "Uri": "http://localhost:9200",
    "IndexName": "aurora-platform"
  },
  "AllowedHosts": "*"
}
